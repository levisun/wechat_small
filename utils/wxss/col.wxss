.ui-container {
    padding-left: 15px;
    padding-right: 15px;
    overflow: hidden;
    min-height: 100vh;
    box-sizing: border-box;
    background-color: #f9f9f9;
}
.ui-container .ui-row {
    margin-left: -15px;
    margin-right: -15px;
    box-sizing: border-box;
}
.ui-container .ui-row:after {
    content: "";
    display: table;
    clear: both;
}
.ui-col-1, .ui-col-2, .ui-col-3, .ui-col-4, .ui-col-5, .ui-col-6, .ui-col-7, .ui-col-8, .ui-col-9, .ui-col-10, .ui-col-11, .ui-col-12 {
    float: left;
    position: relative;
    min-height: 1px;
    padding-right: 15px;
    padding-left: 15px;
    box-sizing: border-box;
}
